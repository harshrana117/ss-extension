{
  "name": "FPWSS",
  "version": "1.0",
  "description": "Take full page web screenshot",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"

  },
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]

}